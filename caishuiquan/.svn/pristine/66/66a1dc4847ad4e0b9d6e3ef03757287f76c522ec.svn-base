<script type="text/javascript">
    
    $(function(){
        if(window.localStorage){
           // alert("support!");
        }else{
            alert("本浏览器不支持部分功能,建议使用其他浏览器：");
        }

        $(".register").click(function(){
            var phone = $(".my_phone").val();
                pwd = $(".user_pwd").val(),
                rpwd = $(".rpwd").val(),
                mpwd = $(".mpwd").val();

            $.ajax({
                timeout:30000,
                url:'login.php',
                data:{
                    phone:phone,
                    pwd:pwd,
                    rpwd:rpwd,
                    mpwd:mpwd,
                    rmd:Math.random()
                },
                tyoe:'post',
                dataType:'json',
                success:function(data){
                    if (data.msg == "yes") {
                        alert("注册成功！");
                        window.location.href = "login.html";
                    }else{
                        alert(data.msg);
                    }
                },
                error:function(){
                    alert("系统繁忙，请稍后");
                }
            })
        })
    })
</script>

