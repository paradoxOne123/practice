                if(data.length == 0){
                	alert("当前服务暂无子类别");
                }else{

                	$.each(data.fwzlb,function(index,a){
                		var fwName = a.fwName
                		    fwVaule = a.fwVaule
                            option = '<option fwzlb-value="'+ fwVaule +'">'+ fwName +'</option>';
                		$("#fwlb-s").append(option);
                	})
                }
            })
		})
        
		//请选择城市
