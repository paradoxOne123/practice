            //alert("change");
            var province = $("#fwarea-1 option:selected").attr("area-value");
            var args = {
            	"time": new Date(),
            	"province": province
            };

            //alert($("#fwarea-2 option:not(frist)").val());
            $.get("data/city.json",args,function(data){
                // alert("getjson");
                if(data.length == 0){
                	alert("当前省份暂无提供服务的城市");
                }else{
                	$("#fwarea-2").show();
                    $('#fwarea-2').find('option:not(:first)').remove();
                	$.each(data.cityList,function(index,a){
                		var cityName = a.cityName
                		    cityValue = a.cityValue
                            option = '<option city-value="'+ cityValue +'">'+ cityName +'</option>';
                		$("#fwarea-2").append(option);
                	})
                }
            })
		})

		//请选择三级地址
