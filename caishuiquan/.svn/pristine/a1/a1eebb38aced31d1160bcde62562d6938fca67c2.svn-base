* {
	padding: 0;
	margin: 0;
	list-style-type: none;
}
.clearfix:after {
	content: '';
	display: block;
	height: 0;
	visibility: hidden;
	clear: both;
}
header {
	width: 100%;
	background: #fff;
}
.top-header {
}
.top-bar-wrap {
	width: 100%;
	position: fixed;
	top: 0;
	background: #fff;
	border-bottom: 1px solid #e3e3e3;
	z-index: 90;
}
.top-bar {
	width: 1200px;
	margin: 0 auto;
	height: 26px;
}
.change-city {
	float: left;
}
.change-city span {
	font-size: 12px;
	line-height: 26px;
	color: #555;
}
.change-city a {
	font-size: 12px;
	line-height: 26px;
	color: #4364A8;
	text-decoration: none;
}
.change-city a:hover {
	text-decoration: underline;
}
.personal-center {
	float: right;
}
.personal-center>a {
	display: block;
	padding: 0 6px;
	float: left;
}
.personal-center>div {
	display: block;
	float: left;
}
.personal-center>a {
	font-size: 12px;
	line-height: 26px;
	color: #4364A8;
	text-decoration: none;
}
.personal-center>div>span {
	position: relative;
	display: inline-block;
	padding: 0 12px;
	font-size: 12px;
	line-height: 26px;
	color: #555;
	cursor: pointer;
	box-sizing: content-box;
	border-left: 1px solid #fff;
	border-right: 1px solid #fff;
	border-bottom: 1px solid #fff;
	z-index: 9;
}
.arrow {
	margin-left: 3px;
	position: relative;
	top: 2px;
	display: inline-block;
	border: 4px solid transparent;
	border-top-color: #555;
}
.personal-center>div {
	position: relative;
}
.personal-center>div>ul {
	display: none;
	position: absolute;
	top: 100%;
	right: 0;
	background: #fff;
	border: 1px solid #e3e3e3;
}
.personal-center>div>ul>li {
	width: 90px;
	padding-left: 10px;
}
.personal-center>div:last-child>ul>li {
	width: 160px;
}
.personal-center>div>ul>li:hover {
	background: #f5f5f5;
}
.personal-center>div>ul>li>a {
	display: block;
	font-size: 12px;
	line-height: 26px;
	color: #4364A8;
	text-decoration: none;
}
.personal-center>div>ul>li>a:hover {
	color: #555;
}
.logo-search {
	position: relative;
	width: 1200px;
	height: 125px;
	margin: 28px auto 0;
}
.logo {
	position: absolute;
	top: 50%;
	transform: translateY(-50%);
	display: block;
	width: 80px;
	height: 80px;
	float: left;
}
.logo img {
	width: 80px;
	height: 80px;
}
.search-bar {
	position: absolute;
	top: 50%;
	right: 0;
	transform: translateY(-50%);
}
.search-bar>* {
	display: block;
	float: left;
}
.search-bar>.search {
	box-sizing: content-box;
	border: 2px solid #2399F4;
	border-radius: 5px;
}
.search .option {
	position: relative;
	float: left;
	width: 70px;
	background: #fff;
	font-size: 14px;
	padding: 10px 10px;
	border-right: 2px solid #2399F4;
	cursor: pointer;
	color: #555;
	text-align: center;
}
.search .option .chose {
	position: absolute;
	top: 100%;
	left: 0;
	width: 100%;
	box-sizing: content-box;
}
.search .option .chose span {
	display: block;
	width: 100%;
	line-height: 28px;
	background: #f9f9f9;
	text-align: center;
}
.search .option .chose span:hover {
	color: rgba(0,0,200,.8);
}
.search input {
	outline: none;
	border: 0;
	width: 280px;
	font-size: 14px;
	padding: 11px 0 11px 10px;
}
.search .button {
	background: #2399F4;
	color: #fff;
	font-size: 14px;
	padding: 13px 15px;
}
.search-bar>span {
	font-size: 12px;
	line-height: 38px;
	color: #999;
	padding: 0 10px;
}
.search-bar>a {
	display: block;
	padding: 0 15px;
	font-size: 14px;
	line-height: 38px;
	color: #fff;
	border-radius: 5px;
	text-decoration: none;
}
.chose {
	z-index: 10;
}
.post-task {
	background: #F87B4F;
}
.register {
	margin-left: 10px;
	background: #4CE357;
}
.hot-search {
	position: absolute;
	left: 0;
	top: 45px;
	font-size: 12px;
	color: #777;
}
.hot-search a {
	padding: 5px;
	display: inline-block;
	margin-left: 5px;
	text-decoration: none;
	font-size: 12px;
	color: #777;
	border-radius: 3px;
}
.hot-search a:hover {
	background: rgba(0,0,255,.4);
	color: #fff;
}
.personal-settings {
	width: 100%;
	margin: 28px auto 0;
	background: #2399F4;
}
.personal-settings>ul {
	width: 1200px;
	margin: 0 auto;
}
.personal-settings>ul:after {
	content: '';
	height: 0;
	clear: both;
	visibility: hidden;
	display: block;
}
.personal-settings>ul>li {
	cursor: pointer;
	float: left;
	opacity: .5;
}
.personal-settings>ul>li>a {
	font-size: 14px;
	line-height: 48px;
	padding: 0 15px;
	color: #fff;
	text-decoration: none;
}
.personal-settings>ul>li>a>img {
	position: relative;
	top: 5px;
	width: 25px;
	margin-right: 5px;
}
.personal-settings .active {
	opacity: 1;
}
.personal-settings>ul>li:first-child {
	font-size: 20px;
	line-height: 48px;
	font-weight: 700;
	color: #fff;
	padding: 0;
	margin-right: 20px;
	text-shadow: 2px 2px 2px rgba(0,0,0,.5);
	opacity: 1;
}
.instant-link {
	position: fixed;
	top: 50%;
	right: 0;
}
.instant-link .icon {
	display: block;
	height: 40px;
	text-decoration: none;
	margin-bottom: 5px;
}
.icon>div {
	padding: 0 10px;
	background: #2399F4;
	border-radius: 10px;
}
.icon>div:hover {
	background: orange;
}
.icon-personal {
	position: absolute;
	top: 0;
	right: 0;
}
.icon-task {
	position: absolute;
	top: 50px;
	right: 0;
}
.icon-order {
	position: absolute;
	top: 100px;
	right: 0;
}
.icon-message {
	position: absolute;
	top: 150px;
	right: 0;
}
.icon-service {
	position: absolute;
	top: 200px;
	right: 0;
}
.icon:hover span {
	display: inline-block;
}
.icon>div>span {
	font-size: 16px;
	color: #fff;
	position: relative;
	top: -12.5px;
	display: none;
}
.icon>div>img {
	width: 25px;
	height: 25px;
	padding: 7.5px 0;
}
.main-content {
	width: 1200px;
	margin: 10px auto 0;
}
.main-content:after {
	content: '';
	display: block;
	visibility: hidden;
	clear: both;
	height: 0;
}
.side-nav {
	width: 20%;
	margin-right: 5%;
	float: left;
	border: 1px solid #e3e3e3;
	box-sizing: border-box;
}
.side-nav-klass p {
	padding: 0 20px;
	font-size: 14px;
	font-weight: 700;
	line-height: 34px;
	background: #f5f5f5;
	border-top: 1px solid #e3e3e3;
	border-bottom: 1px solid #e3e3e3;
	box-sizing: border-box;
	cursor: pointer;
}
.side-nav-klass:first-child p {
	border-top: 0;
}
.side-nav-klass>ul {
	padding: 0 20px;
	background: #fff;
}
.side-nav-klass>ul>li>a {
	font-size: 14px;
	line-height: 28px;
	color: #777;
	text-decoration: none;
}
.side-nav .active p {
	border-left: 5px solid orange;
}
.side-nav .active .hidden-content {
	display: block;
}
.hidden-content {
	display: none;
}
/*主要内容右侧部分*/
.content-right {
	width: 75%;
	float: right;
}
.content-right h3 {
	margin-top: 15px;
	margin-bottom: 20px;
	font-size: 20px;
	line-height: 28px;
	color: #333;
	text-align: center;
	letter-spacing: 10px;
}
.table-left {
	width: 20%;
	float: left;
}
.table-left span {
	display: block;
	text-align: right;
	font-size: 14px;
	line-height: 14px;
	padding: 10px 0;
	color: #555;
	margin-bottom: 10px;
}
.table-left span:last-child {
	margin-top: 130px;
}
.table-right {
	width: 78%;
	margin-left: 2%;
	float: left;
}
.table-right * {
	display: block;
	margin-bottom: 10px;
}
.table-right input {
	padding: 7.5px;
	font-size: 14px;
	line-height: 14px;
	color: #555;
	width: 80%;
	border: 1px solid #e3e3e3;
	border-radius: 5px;
}
.table-right select {
	width: 120px;
	text-align: center;
	-webkit-appearance: none;
	appearance: none;
	padding: 7.5px 30px 7.5px 15px;
	font-size: 14px;
	background: #fff url(../images/arrow-down.png) no-repeat 95% center;
	background-size: 15px;
	border: 1px solid #e3e3e3;
}
.table-right .crew-describe {
	position: relative;
}
.table-right textarea {
	width: 80%;
	height: 130px;
	padding: 10px 7.5px;
	font-size: 14px;
	line-height: 16px;
	border: 1px solid #e3e3e3;
	border-radius: 5px;
	resize: none;
}
.table-right .crew-describe>span {
	display: inline-block;
	position: absolute;
	right: 20%;
	bottom: 0px;
	font-size: 12px;
	line-height: 16px;
	color: #999;
}
.crew-describe>span>span {
	display: inline;
	color: orange;
}
.table-right a {
	overflow: hidden;
	position: relative;
	display: inline-block;
	padding: 10px;
	border: 1px solid #e3e3e3;
	cursor: pointer;
	border-radius: 5px;
}
.table-right a.submit{
	padding: 10px 80px;
    background-color: #2399F4;
    color: #fff;
    margin-left: 180px;
    margin-top: 15px;
}
.table-right a input[type="file"] {
	position: absolute;
	right: 0;
	top: 0;
	border: 0;
	padding: 100px;
	opacity: 0;
}
/*底部部分*/
footer{
	margin-top: 30px;
    width:100%;
    border-top:1px solid #999999;
    text-align: center;
    background-color: #f3f3f3;
    padding:.01%;
}
footer>div {
	margin-top: 10px;
}
footer a,p,div {
	font-size: 12px;
	line-height: 18px;
	color: #666;
	text-decoration: none;
}

