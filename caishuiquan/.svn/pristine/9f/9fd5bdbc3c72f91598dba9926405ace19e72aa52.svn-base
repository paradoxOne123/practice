* {
	padding: 0;
	margin: 0;
	list-style-type: none;
}
/*这里是侧边快捷导航*/
.instant-link {
	position: fixed;
	top: 50%;
	right: 0;
	z-index: 30;
}
.instant-link .icon {
	display: block;
	height: 40px;
	text-decoration: none;
	margin-bottom: 5px;
}
.icon>div {
	padding: 0 10px;
	background: #2399F4;
	border-radius: 10px;
}
.icon>div:hover {
	background: orange;
}
.icon-personal {
	position: absolute;
	top: 0;
	right: 0;
}
.icon-task {
	position: absolute;
	top: 50px;
	right: 0;
}
.icon-order {
	position: absolute;
	top: 100px;
	right: 0;
}
.icon-message {
	position: absolute;
	top: 150px;
	right: 0;
}
.icon-service {
	position: absolute;
	top: 200px;
	right: 0;
}
.icon:hover span {
	display: inline-block;
}
.icon>div>span {
	font-size: 16px;
	color: #fff;
	position: relative;
	top: -12.5px;
	display: none;
}
.icon>div>img {
	width: 25px;
	height: 25px;
	padding: 7.5px 0;
}
/*这里是课程简介*/
.clear-fix:after {
	content: '';
	display: block;
	height: 0;
	visibility: hidden;
	clear: both;
}
.lesson-thumbnail {
	width: 1200px;
	margin: 20px auto 0;
	background: #fff;
	box-shadow: 0 0 10px rgba(0,0,0,.2);
	border-radius: 5px;
	box-sizing: border-box;
	padding: 10px;
}
.lesson-banner {
	width: 58%;
	margin-right:2%; 
	float: left;
}
.lesson-banner img {
	width: 100%;
	border-radius: 5px;
}
.lesson-info {
	width: 40%;
	height: 380px;
	float: left;
	position: relative;
}
.lesson-info>h3 {
	font-size: 24px;
	margin-top: 20px;
	font-weight: normal;
}
.lesson-info .lesson-teacher {
	margin-top: 20px;
}
.lesson-info .lesson-teacher p {
	text-indent: 0;
	font-size: 18px;
	line-height: 26px;
	font-weight: 700;
}
.lesson-info .lesson-teacher p span{
	font-weight: normal;
}
.lesson-info .lesson-rate {
	position: absolute;
	left: 0;
	bottom: 54px;
}
.lesson-info .lesson-rate img {
	width: 20px;
}
.lesson-info .lesson-rate span {
	font-size: 14px;
	line-height: 20px;
	color: #777;
	position: relative;
	top: -4px;
}
.lesson-info .buy-now {
	position: absolute;
	bottom: 0;
	left: 0;
	width: 100%;
}
.lesson-info .buy-now a {
	display: block;
	width: 45%;
	float: left;
	font-size: 16px;
	line-height: 34px;
	text-align: center;
	border-radius: 5px;
	color: #fff;
	text-decoration: none;
}
.lesson-info .buy-now a:first-child {
	margin-right: 10%;
	background: #2399F4;
}
.lesson-info .buy-now a:last-child {
	background: orange;
}
/*这里是课程详情*/
.lesson-detail {
	width: 1200px;
	margin: 20px auto 0;
	border-radius: 5px;
	box-shadow: 0 0 10px rgba(0,0,0,.2);
}
.lesson-detail .detail-left {
	padding: 0 10px;
	width: 68%;
	float: left;
	margin-right: 2%;
	box-sizing: border-box;
	position: relative;
}
.detail-left>ul,.detail-right>ul {
	margin-top: 10px;
}
.detail-left>ul:after,.detail-right>ul:after {
	content: '';
	height: 0px;
	display: block;
	border-bottom:3px dashed #e3e3e3;
	clear: both;
	position: relative;
	top: -3px;
	z-index: -1;
}
.detail-left>ul>li,.detail-left>ul>li {
	display: inline-block;
	float: left;
	cursor: pointer;
}
.detail-left ul li span,.detail-right ul li span {
	display: inline-block;
	font-size: 18px;
	line-height: 38px;
	padding: 0 20px;
	color: #555;
	border-bottom: 3px solid transparent;
}
.detail-left ul>.active span {
	border-color: #00a85f;
}
.details-content .active {
	display: block;
}
.details-content>div {
	display: none;
}
.details-content h3 {
	font-size: 30px;
	line-height: 36px;
	font-weight: 100;
	letter-spacing: 5px;
	margin: 10px 0;
	padding-left: 20px;
	border-left: 3px solid #e3e3e3;
	color: #555;
}
.content-info>h4,p {
	text-indent: 2em;
	margin-top: 10px;
	color: #777;
}
.content-info h4 {
	font-size: 16px;
	line-height: 24px;

}
.content-info p {
	font-size: 16px;
	line-height: 22px;
}
.content-list>ul>li {
	border-bottom: 1px solid #e3e3e3;
}
.content-list>ul>li>a {
	display: block;
	padding: 5px 0;
}
.content-list>ul>li>a>div:first-child {
	float: left;
}
.content-list>ul>li>a>div:nth-child(2){
	float: right;
}
.content-list>ul>li>a>div>img {
	width: 25px;
	opacity: 0.2;
	position: relative;
	top: 5px;
	margin-right: 10px;
}
.content-list>ul>li>a>div>span {
	font-size: 16px;
	line-height: 30px;
	color: #555;
	padding:0;
}
.content-list .for-free {
	padding: 5px 10px;
	font-size: 12px;
	line-height: 12px;
	background: #2399F4;
	border-radius: 5px;
	color: #fff !important;
	border: 0;
}
.content-list>ul>li>a>div:nth-child(2) span {
	font-size: 14px;
	line-height: 30px;
	color: #777;
}
.content-list>ul>li>a:hover>div:first-child img {
	opacity: 1;
}
.content-list>ul>li>a:hover>div:first-child span {
	color: #00a85f;
}
.from-students {
	margin-bottom: 20px;
}
.from-students>div {
	margin-bottom: 10px;
}
.from-students img {
	margin-left: 10px;
	float: left;
	width: 80px;
	height: 80px;
	border-radius: 50px;
}
.from-students>div>div {
	width: 686px;
	float: left;
	margin-left: 20px;
}
.from-students>div>div span {
	font-size: 14px;
	margin-bottom: 10px;
	color: #777;
	margin-right: 10px;
}
.from-students>div>div p {
	height: 100px;
	border: 1px dashed #e3e3e3;
	text-indent: 0;
	background: #f9f9f9;
	font-size: 14px;
	line-height: 20px;
	border-radius: 5px;
	box-sizing: border-box;
	padding: 10px;
}
.my-quest {
	margin: 20px 0;
}
.my-quest img{
	margin-left: 10px;
	float: left;
	width: 80px;
	height: 80px;
	border-radius: 50px;
}
.my-quest textarea {
	width: 686px;
	height: 100px;
	float: left;
	margin-left: 20px;
	border: 1px dashed #e3e3e3;
 	resize : none;
 	background: #f9f9f9;
 	border-radius: 5px;
 	font-size: 14px;
	line-height: 20px;
	outline: none;
	padding: 10px;
	box-sizing: border-box;
 }
.my-quest>span {
	float: left;
	margin-left: 110px;
	margin-top: 10px;
	font-size: 12px;
	line-height: 30px;
	color: #777;
}
.my-quest>span>var {
	color: orange;
}
.my-quest input {
	float: right;
	margin-top: 10px;
	padding: 0 20px;
	font-size: 14px;
	line-height: 30px;
	color: #fff;
	border-radius: 5px;
	background: rgba(0,168,95,.6);
	border: 0;
	cursor: pointer;
}
.my-quest input:hover {
	background: #00a85f;
}
.detail-right {
	width: 30%;
	float: left;
	padding: 0 10px;
	box-sizing: border-box;
}
.detail-right ul li span {
	border-color: #00a85f;
}
.detail-right>div>img{
	width: 100px;
	height: 100px;
	border-radius: 50px;
	float: left;
	margin-left: 10px;
}
.detail-right>div>p {
	text-indent: 0;
	margin-left: 120px;
	font-size: 16px;
	color: #333;
	line-height: 36px;
}
.detail-right>div>span {
	display: block;
	margin-left: 120px;
	color: #555;
	font-size: 14px;
	line-height: 36px;
}
.detail-right>p {

	font-size: 14px;
	line-height: 18px;
	margin: 10px;
	color: #555;
}
.other-lesson {
	padding: 10px;
}
.other-lesson h4 {
	font-size: 16px;
	margin: 10px 0;
	font-weight: 100;
	color: #555;
}
.other-lesson a {
	display: block;
	padding: 10px;
	margin-bottom: 10px;
}
.other-lesson a:hover {
	box-shadow: 0 0 10px rgba(0,0,0,.3);
}
.other-lesson a>img {
	width: 50%;
	float: left;
}
.other-lesson a div {
	width: 45%;
	height: 107px;
	margin-left: 5%;
	float: left;
	position: relative;
}
.other-lesson a div p {
	text-indent: 0;
	font-size: 14px;
	line-height: 18px;
	margin-top: 5px;
}
.other-lesson a div img {
	position: absolute;
	bottom: 5px;
	left: 0;
	width: 14px;
}
.other-lesson a div span {
	font-size: 14px;
	position: absolute;
	bottom: 5px;
	left: 20px;
	color: #999;
}





