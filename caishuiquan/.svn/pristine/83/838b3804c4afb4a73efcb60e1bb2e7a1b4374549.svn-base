.box{width: 1000px;margin: 0 auto;}
.w_fluid{width: 100%;}

.top10{margin-top: 10px;}
.bottom10{margin-bottom: 10px;}
.left10{margin-left: 10px;}
.right10{margin-right: 10px;}
.ptop10{padding-top: 10px;}
.pbottom10{padding-bottom: 10px;}
.pleft10{padding-left: 10px;}
.pright10{padding-right: 10px;}

.left{float: left;}
.right{float: right;}
.clear{clear: both;overflow: hidden;zoom:1;}

.cslink{color: #f90!important;}
.img-auto{width: 100%;height: auto;}
.f12{font-size: 12px!important;}
i.iconfont:hover{color: #f90!important;}

a:hover{text-decoration: none!important;}
a{font-size: 12px;text-decoration: none;}

.sjbox{
	background-color: #fff;
    color: #5a5a5a;
}
.sns-maincontent .sjbox{
    border: 1px solid #ededed;
    /*box-shadow: 0 0 5px rgba(0,0,0,.3);*/
}
.sjcont{
	padding: 20px 20px 10px;
	border-bottom: 1px solid #F3F3F3;
}
.sjcont .cont{

}
.sjcont .cont .touxiang{
    width: 50px;
    display: inline-block;
    float: left;
}
.sjcont .cont .rct{
    margin-left: 60px;
    /*display: inline-block;
        margin-top: -60px;*/
}
.u-tit{
    padding: 5px 0 0;
    /*margin-bottom: 0px;*/
    line-height: 25px;
}
.u-tit a.u-name{
	color: #333;
	font-weight: bold;
}
.u-tit a img{
    width: 16px;
    height: 16px;
}
.sjcont .cont .rct .u-time a{
    color: #808080;
    line-height: 20px;
}
.sjcont .cont .rct .u-word{
    padding: 1px 0 3px;
    overflow: hidden;
    line-height: 23px;
    word-wrap: break-word;
    overflow: hidden;
    text-overflow: ellipsis;
    /* white-space: nowrap; */
    width: 500px;
    height: 75px;
    font-size: 12px;
    color: #5a5a5a;
}
.sjcont .cont .rct .u-img img{
	width: 100%;
	height: auto;
}
.sjbox .sj-botm-nav{
    
}
.sjbox .sj-botm-nav ul li{
	float: left;
	height: 39px;
    line-height: 39px;
    width: 25%;
    text-align: center;
    position: relative;
}
.sjbox .sj-botm-nav ul li span.arrow-t{
	left: 70px;
    border: 8px solid transparent;
    border-bottom: 8px solid #f2f2f5;
    width: 0;
    height: 0px;
    position: absolute;
    bottom: 0px;
    display: none;
}
.sjbox .sj-botm-nav ul li a span{
    color: #808080;
    display: inline-block;
    width: 100%;
    border-right: 1px solid #ddd;
    line-height: 22px;
    margin-left: -1px;
    font-size: 12px;
}
.sjbox .sj-botm-nav ul li a span i{

}
.sjbox .sj-botm-nav ul li a span:hover{
	color: #f90;
}
.sjbox .sj-botm-nav ul li a span em{
    font-style: normal;
}
.sjbox .sj-botm-nav .commentbox{

}
.sjbox .sj-botm-nav .comment{
	display: none;
}
.sjbox .sj-botm-nav .comment .cont{
	padding: 15px 20px 0px 30px;
	background-color: #f2f2f5;
}
.sjbox .sj-botm-nav .commentbox{

}
.sjbox .sj-botm-nav .commentbox .tx{
    width: 30px;
    height: 30px;
    position: relative;
}
.sjbox .sj-botm-nav .commentbox .rct{
    margin-left: 50px;
    padding-bottom: 25px;
    border-bottom: 1px solid #ddd;
}
.sjbox .sj-botm-nav .commentbox .rct .comt-edit textarea{
    margin: 0px;
    padding: 5px 2px 0px 6px;
    border-style: solid;
    border-width: 1px;
    font-size: 12px;
    word-wrap: break-word;
    line-height: 18px;
    overflow-y: scroll;
    outline: none;
    height: 23px;
    border-color: #cccccc;
    width: 98%;
}
.sjbox .sj-botm-nav .commentbox .rct .submit a{
    height: 28px;
    line-height: 29px;
    width: 60px;
    background-color: #ddd;
    color: #fff;
    text-align: center;
    display:inline-block;
}
.sjbox .sj-botm-nav .commentbox .rct .comt-subm{
	padding-bottom: 15px;
    border-bottom: 1px solid #ddd;
}
.sjbox .sj-botm-nav .commentbox .rct .comtcont{
    padding: 15px 0px;
}
.sjbox .sj-botm-nav .commentbox .rct .comtcont .comt-tit-list ul li{
	margin:0px 5px;
	width: auto;
	border-right: 1px solid #ddd;
	padding:0px 8px;
	line-height: 14px;
    height: 14px;
}
.sjbox .sj-botm-nav .commentbox .rct .comtcont .comt-tit-list ul li a{
    color: #333;
    font-size: 12px;
}
.sjbox .sj-botm-nav .commentbox .rct .comtcont .comt-count{font-size: 12px;}
.sjbox .sj-botm-nav .commentbox .rct .comtcont .comt-art{
	padding: 5px 0 7px;
	border-bottom: 1px solid #E4E4E4;
    position: relative;
}
.sjbox .sj-botm-nav .commentbox .rct .comtcont .comt-art .tx{

}
.sjbox .sj-botm-nav .commentbox .comt-art .rct{
    padding-bottom: 0px;
    border-bottom: 0px;
}
.sjbox .sj-botm-nav .commentbox .comt-art .rct .comt-word{
	line-height: 25px;
	font-size: 12px;
}
.sjbox .sj-botm-nav .commentbox .comt-art .rct .comt-info .comt-time,.comt-info .comt-act a{
    color: #808080;
    font-size: 12px;
}
.comt-info .comt-act a{
        padding: 0px 5px;
    border-right: 1px solid #ddd;
}
.comt-info .comt-act a.jubao{
	display: none;
}
.comt-info .comt-act a.dz{
	border-right: 0px;
}
.sjbox .sj-botm-nav .commentbox .rct .comtcont .comt-art:hover a.jubao{
	display: inline-block;
}
.sjbox .sj-botm-nav .commentbox .comt-art .rct .comt-replay{
    position: absolute;
    background-color: #fff;
    padding: 15px 10px 0px 10px;
    width: 450px;
    z-index: 10;
    right: 0px;
    top: 65px;
    box-shadow: 0 4px 20px 1px rgba(0,0,0,0.2);
    display: none;
}
.sjbox .sj-botm-nav .commentbox .comt-art .rct .comt-replay span.arrow-t{
        border: 8px solid transparent;
    border-bottom: 8px solid #fff;
    width: 0;
    height: 0px;
    position: absolute;
    bottom: 0px;
    top: -15px;
    right: 55px;
    display: block;
    left: auto;
}
.sjbox .sj-botm-nav .commentbox .comt-art .rct .comt-replay .comt-subm {
    border:0px;
}