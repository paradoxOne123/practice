<script type="text/javascript">
	
	$(function(){
        
        //请选择服务类别
		$("#fwlb-l").change(function(){
			
            var fwlb = $(this).val();
            var swlb = $("#fwlb-l option:selected").attr("swlb-value");
            //alert($("#fwlb-l option:selected").attr("swlb-value"));
            var args = {
            	"time": new Date(),
            	"swlb": swlb
            };

            $("#fwlb-s option:not(frist)").remove();
            $.get("data/fwlb.json",args,function(data){

                if(data.length == 0){
                	alert("当前服务暂无子类别");
                }else{

                	$.each(data.fwzlb,function(index,a){
                		var fwName = a.fwName
                		    fwVaule = a.fwVaule
                            option = '<option fwzlb-value="'+ fwVaule +'">'+ fwName +'</option>';
                		$("#fwlb-s").append(option);
                	})
                }
            })
		})
        
		//请选择城市
		$("#fwarea-1").change(function(){
            //alert("change");
            var province = $("#fwarea-1 option:selected").attr("area-value");
            var args = {
            	"time": new Date(),
            	"province": province
            };

            //alert($("#fwarea-2 option:not(frist)").val());
            $.get("data/city.json",args,function(data){
                // alert("getjson");
                if(data.length == 0){
                	alert("当前省份暂无提供服务的城市");
                }else{
                	$("#fwarea-2").show();
                    $('#fwarea-2').find('option:not(:first)').remove();
                	$.each(data.cityList,function(index,a){
                		var cityName = a.cityName
                		    cityValue = a.cityValue
                            option = '<option city-value="'+ cityValue +'">'+ cityName +'</option>';
                		$("#fwarea-2").append(option);
                	})
                }
            })
		})

		//请选择三级地址
		$("#fwarea-2").change(function(){
            //alert("change");
            var zone = $("#fwarea-2 option:selected").attr("city-value");
            var args = {
            	"time": new Date(),
            	"zone": zone
            };
             
            //alert($("#fwarea-2 option:not(frist)").val());
            $.get("data/zone.json",args,function(data){
                // alert("getjson");
                if(data.length == 0){
                	alert("当前省份暂无提供服务的城市");
                }else{
		            $("#fwarea-3").show();
		            $('#fwarea-3').find('option:not(:first)').remove();
                	$.each(data.zoneList,function(index,a){
                		var zoneName = a.zoneName
                		    zoneValue = a.zoneValue
                            option = '<option zone-value="'+ zoneValue +'">'+ zoneName +'</option>';
                		$("#fwarea-3").append(option);
                	})
                }
            })
		})

		//填写完表单 点击下一步
        $(".fw-sub1").click(function(){
        	event.preventDefault();
        	//var str = JSON.stringify($("#form1").serializeArray());
        	var str = $("#form1").serializeArray();
        	//alert(str[0].value);

        	jQuery.each( str, function(i, a){
			   // alert(i+a.value);
			});
        	//alert(str.value);

        	//提交到验证php 返回验证成功后 以下
            $(".rw-form").hide();
            $(".confirm-form").show();
 
            $(".rw-progress ul li").eq(1).addClass("step-active");

            var tableCont = '';

            tableCont += '<tbody><tr><td width="150">服务类别</td><td>'+ str[0].value + ',' +str[1].value +'</td></tr><tr><td width="150">服务售价</td><td>￥'+ str[7].value +'.00</td></tr><tr><td width="150">所属地区</td><td>'+ str[2].value+ ',' + str[3].value+ '市,' + str[4].value+'</td></tr><tr><td width="150">服务名称</td><td>'+ str[5].value +'</td></tr><tr><td width="150">服务详情</td><td>'+ str[6].value +'</td></tr></tbody>';
            $("#confirm-table").html(' ');
            $("#confirm-table").append(tableCont);   
        })

        //点击确认并提交 
        $(".fw-sub2").click(function(){
        	event.preventDefault();

        	//提交数据 JSON.stringify($("#form1").serializeArray()) 成功后 以下
            
            $(".rw-progress ul li").eq(2).addClass("step-active");
            $(".confirm-form").hide();
            $(".fw-success").show();
        })

        $(".backward").click(function(){
        	$(".rw-form").show();
            $(".confirm-form").hide();
        })

	})

</script>

<script type="text/javascript">

/*
* 服务器地址,成功返回,失败返回参数格式依照jquery.ajax习惯;
