
.rw{
    border: 1px solid #ededed;
    background-color: #fff;
}
.rw .rw-progress{
    padding-top: 30px;
}
.rw .rw-progress ul li{
	float: left;
	width: 33.3%;
	border-top: 5px solid #d5d5d5;
	position: relative;
}
.rw .rw-progress ul li span.step-icon{
    width: 25px;
    height: 25px;
    display: inline-block;
    position: absolute;
    z-index: 1;
    top: -15px;
    left: 50%;
    margin-left: -12.5px;
    background: #d5d5d5;
    line-height: 25px;
    border-radius: 50%;
    text-align: center;
    color: #fff;
}
.rw .rw-progress ul li .step-text{
    padding-top: 15px;
    text-align: center;
}
.step-active{border-color: #3f8be9!important;}
.step-active span.step-icon{
    background-color: #3f8be9!important;
}
.step-active .step-text{
	color: #3f8be9;
}

.rw .rw-form{
    padding: 35px 0px 15px 150px;
}
.rw .rw-form .form-group{
	margin-bottom: 15px;
}
.rw .rw-form .form-group label{
    color: #333;
    font-weight: bold;
    display: inline-block;
    padding-right: 15px;

}
.rw .rw-form .form-group label i{color: #f00;}
.rw .rw-form .form-group select,.rw .rw-form .form-group input[type="text"]{
    /*appearance:none;
    -moz-appearance:none;
    -webkit-appearance:none;*/
    border: solid 1px #ddd;
    width: 280px;
    height: 35px;
    border-radius: 3px;
    margin-right: 10px;
    font-size: 12px;
}
.rw .rw-form .form-group label.notice{
	color: #666;
	font-weight: 100;
}
.rw .rw-form .form-group textarea{
	border-color:#CCCCCC #999999 #999999 #CCCCCC;
    border-style:solid;
    border-width:1px;
    height:160px;
    margin:10px auto;
	outline-color:-moz-use-text-color;
	outline-style:none;
	outline-width:medium;
    padding:10px 15px;
    width:600px;
    float: left;
}
.rw .rw-form .form-area select{width: 200px;}
.rw .rw-form .form-group .sell-price{
    display: inline-block;
    position: relative;
}
.rw .rw-form .form-group .sell-price input{
    padding-left: 40px;
    box-sizing: border-box;
}
.rw .rw-form .form-group .sell-price span{
    display: inline-block;
    width: 35px;
    height: 35px;
    line-height: 35px;
    text-align: center;
    position: absolute;
    background-color: #ddd;
}
.rw .form-group button{
	display: inline-block;
    width: 100px;
    height: 35px;
    background-color: #3f8be9;
    border: none;
    color: #fff;
    border-radius: 3px;
    cursor: pointer;
}
#box{
	width: 700px;
   /* height: 400px;*/
    display: inline-block;
    float: right;
    margin-right: 65px;
}

/****/
.rw .confirm-form{padding-bottom: 20px;}
.rw .confirm-form table{
	width: 99%;
    table-layout:fixed; 
	empty-cells:show; 
	border-collapse: collapse; 
	margin:0 auto; 
	border:1px solid #cad9ea; 
	color:#666; 
}
.rw .confirm-form table td,.rw .confirm-form table th{ 
	border:1px solid #cad9ea; 
	padding: 0.5em;
    height: 30px;
    line-height: 18px;
	height: 30px;
    word-wrap:break-word;
    word-break:break-all;
} 
.rw .fw-success{
	width: 100%;
    height: 300px;
    padding: 50px;
}
.rw .fw-success p{
	color: #3c763d;
	margin: 15px 0px;
    font-size: 1.5em;
}
.rw .fw-success a{
    font-size: 1.1em;
    color: #f90;
}