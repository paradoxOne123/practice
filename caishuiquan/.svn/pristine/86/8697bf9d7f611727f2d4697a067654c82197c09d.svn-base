    border-bottom: 1px solid #8DC7D4;
}

/**中间部分 主要内容**/
.sj_maincont .middle-cont{
    display: inline-block;
    width: 600px;
    margin-left: 150px;
    float: left;
}
/**输入框**/
.sj_maincont .middle-cont .send-box,.send-box{
    padding: 15px 10px 12px;
    background-color: #fff;
}
.sj_maincont .send-box .send-title,.send-box .send-title{
	height: 21px;
	line-height: 21px;
	position: relative;
}
.sj_maincont .send-box .send-title .tit,.send-box .send-title .tit{
    color: #2399f4;
    font-weight: bold;
    font-size: 14px;
}
.sj_maincont .send-box .send-title .letter-count,.send-box .send-title .letter-count{
	position: absolute;
	right: 0;
	display: none;
}
.sj_maincont .send-box .send-title .letter-count .cot,.send-box .send-title .letter-count .cot{
	font-size: 16px;
	font-weight: bold;
	color: #f90;
	/*display: none;*/

}
.sj_maincont .send-box .send-input,.send-box .send-input{
    position: relative;
    border: 1px solid #ededed;
    height: 68px;
    margin: 4px 0 0;
    padding: 5px;
}
.sj_maincont .send-box .send-input textarea,.send-box .send-input textarea{
    width: 100%;
    height: 68px;
    margin: 0px;
    padding: 0px;
    border-style: none;
    border-width: 0px;
    font-size: 14px;
    word-wrap: break-word;
    line-height: 18px;
    overflow-y: auto;
    overflow-x: hidden;
    outline: none;
}
.sj_maincont .send-box .send-botm,.send-box .send-botm{
    line-height: 25px;
}
/**图片上传框**/
.sj_maincont .send-box .send-botm .show-upload,.send-box .send-botm .show-upload{
    width: 250px;
    position: absolute;
    padding: 15px;
    background-color: #fff;
    border: 1px solid #ededed;
    z-index: 10;
    top: 170px;
    box-shadow: 0 4px 20px 1px rgba(0,0,0,0.2);
    display: none;
}
.sj_maincont .send-box .send-botm .show-upload p,.send-box .send-botm .show-upload p{
    margin-bottom: 12px;
    font-weight: bold;
    color: #333;
}
.sj_maincont .send-box .send-botm .show-upload input,.send-box .send-botm .show-upload input{

}
