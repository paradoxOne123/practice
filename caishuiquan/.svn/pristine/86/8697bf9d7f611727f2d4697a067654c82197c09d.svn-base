    width: 100%;
    height: auto;
}

.sj_maincont .send-box .send-botm a,.send-box .send-botm a{
    color: #333;
    display: inline-block;
    margin-right: 10px;
} 
.sj_maincont .send-box .send-botm a i,.send-box .send-botm a i{
   color: #f90;
   display: inline-block;
   margin-right: 5px;
}
.sj_maincont .send-box .send-botm .submit a,.send-box .send-botm .submit a{
    height: 28px;
    line-height: 29px;
    width: 60px;
    background-color: #ddd;
    color: #fff;
    text-align: center;
}
