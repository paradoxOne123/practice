* {
	padding: 0;
	margin: 0;
	list-style-type: none;
}
header {
	width: 100%;
	background: #fff;
}
.top-header {
}
.top-bar-wrap {
	width: 100%;
	position: fixed;
	top: 0;
	background: #fff;
	border-bottom: 1px solid #e3e3e3;
	z-index: 90;
}
.top-bar {
	width: 1200px;
	margin: 0 auto;
	height: 26px;
}
.change-city {
	float: left;
}
.change-city span {
	font-size: 12px;
	line-height: 26px;
	color: #555;
}
.change-city a {
	font-size: 12px;
	line-height: 26px;
	color: #4364A8;
	text-decoration: none;
}
.change-city a:hover {
	text-decoration: underline;
}
.personal-center {
	float: right;
}
.personal-center>a {
	display: block;
	padding: 0 6px;
	float: left;
}
.personal-center>div {
	display: block;
	float: left;
}
.personal-center>a {
	font-size: 12px;
	line-height: 26px;
	color: #4364A8;
	text-decoration: none;
}
.personal-center>div>span {
	position: relative;
	display: inline-block;
	padding: 0 12px;
	font-size: 12px;
	line-height: 26px;
	color: #555;
	cursor: pointer;
	box-sizing: content-box;
	border-left: 1px solid #fff;
	border-right: 1px solid #fff;
	border-bottom: 1px solid #fff;
	z-index: 9;
}
.arrow {
	margin-left: 3px;
	position: relative;
	top: 2px;
	display: inline-block;
	border: 4px solid transparent;
	border-top-color: #555;
}
.personal-center>div {
	position: relative;
}
.personal-center>div>ul {
	display: none;
	position: absolute;
	top: 100%;
	right: 0;
	background: #fff;
	border: 1px solid #e3e3e3;
}
.personal-center>div>ul>li {
	width: 90px;
	padding-left: 10px;
}
.personal-center>div:last-child>ul>li {
	width: 160px;
}
.personal-center>div>ul>li:hover {
	background: #f5f5f5;
}
.personal-center>div>ul>li>a {
	display: block;
	font-size: 12px;
	line-height: 26px;
	color: #4364A8;
	text-decoration: none;
}
.personal-center>div>ul>li>a:hover {
	color: #555;
}
.logo-search {
	position: relative;
	width: 1200px;
	height: 125px;
	margin: 28px auto 0;
}
.logo {
	position: absolute;
	top: 50%;
	transform: translateY(-50%);
	display: block;
	width: 80px;
	height: 80px;
	float: left;
}
.logo img {
	width: 80px;
	height: 80px;
}
.search-bar {
	position: absolute;
	top: 50%;
	right: 0;
	transform: translateY(-50%);
}
.search-bar>* {
	display: block;
	float: left;
}
.search-bar>.search {
	box-sizing: content-box;
	border: 2px solid #2399F4;
	border-radius: 5px;
}
.search .option {
	position: relative;
	float: left;
	width: 70px;
	background: #fff;
	font-size: 14px;
	padding: 10px 10px;
	border-right: 2px solid #2399F4;
	cursor: pointer;
	color: #555;
	text-align: center;
}
.search .option .chose {
	position: absolute;
	top: 100%;
	left: 0;
	width: 100%;
	box-sizing: content-box;
}
.search .option .chose span {
	display: block;
	width: 100%;
	line-height: 28px;
	background: #f9f9f9;
	text-align: center;
}
.search .option .chose span:hover {
	color: rgba(0,0,200,.8);
}
.search input {
	outline: none;
	border: 0;
	width: 280px;
	font-size: 14px;
	padding: 11px 0 11px 10px;
}
.search .button {
	background: #2399F4;
	color: #fff;
	font-size: 14px;
	padding: 13px 15px;
}
.search-bar>span {
	font-size: 12px;
	line-height: 38px;
	color: #999;
	padding: 0 10px;
}
.search-bar>a {
	display: block;
	padding: 0 15px;
	font-size: 14px;
	line-height: 38px;
	color: #fff;
	border-radius: 5px;
	text-decoration: none;
}
.chose {
	z-index: 10;
}
.post-task {
	background: #F87B4F;
}
.register {
	margin-left: 10px;
	background: #4CE357;
}
.hot-search {
	position: absolute;
	left: 0;
	top: 45px;
	font-size: 12px;
	color: #777;
}
.hot-search a {
	padding: 5px;
	display: inline-block;
	margin-left: 5px;
	text-decoration: none;
	font-size: 12px;
	color: #777;
	border-radius: 3px;
}
.hot-search a:hover {
	background: rgba(0,0,255,.4);
	color: #fff;
}
.nav {
	border-bottom: 2px solid #2399f4;
	position: relative;
}
.main-nav {
	width: 1200px;
	margin: 0 auto;
}
.main-nav:after {
	content: "";
	display: block;
	height: 0;
	visibility: hidden;
	clear: both;
}
.main-nav a {
	text-decoration: none;
}
.main-nav>.klass {
	float: left;
	color: #999;
}
.main-nav>.klass>a {
	color: #555;
	display: block;
	padding: 0 30px;
	font-size: 16px;
	line-height: 38px;
}
.main-nav>.klass:first-child a {
	color: #fff;
	background: #2399F4;
	border-top-left-radius: 5px;
	border-top-right-radius: 5px;
}
.main-nav>.klass:hover>a {
	color: #fff;
	background: #2399F4;
	border-top-left-radius: 5px;
	border-top-right-radius: 5px;
}
.main-nav>.klass>.sub-nav {
	display: none;
	position: absolute;
	top: 100%;
	left: 0;
	margin-top: 2px;
	width: 100%;
	background: #fff;
	border-bottom: 1px solid #f5f5f5;
	box-shadow: 0 2px 5px rgba(0,0,0,.1);
	text-align: center;
	z-index: 10;
}
.main-nav>.klass>.sub-nav>.content-box {
	display: inline-block;
	padding:0 50px 0 10px;
	margin: 30px 0;
	vertical-align: top;
	text-align: left;
	border-left: 1px solid #f5f5f5;
}
.main-nav>.klass>.sub-nav>.content-box>h4 {
	font-size: 14px;
	line-height: 38px;
	margin-bottom: 10px;
	color: #555;
}
.main-nav>.klass>.sub-nav>.content-box>ul>li>a {
	font-size: 14px;
	line-height: 28px;
	color: #555;
}
.main-nav>.klass>.sub-nav>.content-box>ul>li>a:hover {
	text-decoration: underline;
}
.instant-link {
	position: fixed;
	top: 50%;
	right: 0;
}
.instant-link .icon {
	display: block;
	height: 40px;
	text-decoration: none;
	margin-bottom: 5px;
}
.icon>div {
	padding: 0 10px;
	background: #2399F4;
	border-radius: 10px;
}
.icon>div:hover {
	background: orange;
}
.icon-personal {
	position: absolute;
	top: 0;
	right: 0;
}
.icon-task {
	position: absolute;
	top: 50px;
	right: 0;
}
.icon-order {
	position: absolute;
	top: 100px;
	right: 0;
}
.icon-message {
	position: absolute;
	top: 150px;
	right: 0;
}
.icon-service {
	position: absolute;
	top: 200px;
	right: 0;
}
.icon:hover span {
	display: inline-block;
}
.icon>div>span {
	font-size: 16px;
	color: #fff;
	position: relative;
	top: -12.5px;
	display: none;
}
.icon>div>img {
	width: 25px;
	height: 25px;
	padding: 7.5px 0;
}
.unslider-wrap {
	position: relative;
}
.banner { 
	width: 1200px;
	margin: 0 auto;
	position: relative;
	overflow: hidden; 
}
.banner ul li { 
	float: left;
	list-style-type: none;
	overflow: hidden;
}
.banner ul li img {
	width: 100%;
}
.hotpng {
	display: block;
	position: absolute;
	top: -7px;
	left: -7px;
	z-index: 20;
	width: 80px;
}

.dots {
	text-align: center;
}
.dots>li {
	position: relative;
	top: -20px;
	display: inline-block;
	width: 10px;
	height: 10px;
	background: #eee;
	margin: 0 10px;
	cursor: pointer;
	border: 2px solid #eee;
	border-radius: 10px;
}
.dots>.active {
	background: #2399F4;
}
.unslider-arrow {
	display: none;
}
.klass-list {
	width: 100%;
}
.klass-list>div {
	width: 1200px;
	margin: 10px auto 0;
	box-sizing: border-box;
}
.klass-list>div:after {
	margin-top: -3px;
	content: "";
	display: block;
	width: 1200px;
	height: 3px;
	background: #f5f5f5;
}
.klass-nav-main:after {
	content: '';
	display: block;
	height: 0;
	visibility: hidden;
	clear: both;	
}
.klass-nav-main li {
	position: relative;
	float: left;
	border-bottom: 3px solid transparent;
}
.klass-nav-main>li>a {
	display: block;
	padding: 0 15px;
	font-size: 14px;
	line-height: 44px;
	color: #555;
	text-decoration: none;
}
.klass-nav-main>li:hover>div{
	display: block;
}
.klass-nav-sub {
	display: none;
	position: absolute;
	left: -10%;
	top: 100%;
	margin-top: -8px;
	width: 120%;
	box-sizing: border-box;
	z-index: 10;
}
.klass-nav-sub:before {
	position: relative;
	top: -5px;
	left: calc(48%);
	content: '';
	width: 5px;
	height: 5px;
	border: 5px solid transparent;
	border-bottom-color: #fff;
}
.klass-nav-sub a {
	display: block;
	font-size: 14px;
	line-height: 30px;
	color: #555;
	text-decoration: none;
	text-align: center;
	background: #fff;
	box-shadow: 0 3px 5px rgba(0,0,0,.1);
}
.klass-nav-sub a:hover {
	font-weight: 700;
	color: #fff;
	background: orange;
}
.filtering {
	width: 100%;
}
.filtering-klass {
	margin: 10px auto 0;
	width: 1200px;
	padding: 10px 20px;
	border: 1px solid #e3e3e3;
	box-sizing: border-box;
}
.filtering-klass>div {
	padding: 5px 0;
	border-bottom: 1px dashed #e3e3e3;
	overflow: hidden;
}
.filtering-klass>div:last-child {
	border: 0;
}
.filtering-klass>div>p {
	font-size: 16px;
	line-height: 34px;
	font-weight: 700;
	width: 10%;
	float: left;
}
.filtering-klass>div>div {
	width: 90%;
	float: left;
}
.filtering-klass>div>div>span {
	position: relative;
	top: 2px;
	margin-right: 10px;
	display: inline-block;
	font-size: 14px;
	line-height: 28px;
	padding: 0 10px;
	color: #777;
	border: 1px solid #f5f5f5;
	border-radius: 10px;
	cursor: pointer;
	box-sizing: border-box;
}
.filtering-klass .active {
	background: #2399F4;
	border-color: #2399f4;
	color: #fff;
	font-weight: 700;
}
.task-detail {
	width: 1200px;
	margin: 10px auto 0;
	border-top: 2px solid #2399F4;
	border-left: 1px solid #e3e3e3;
	border-right: 1px solid #e3e3e3;
	box-sizing: border-box;
}
.task-header {
	overflow: hidden;
	padding: 0 10px;
}
.task-header h4 {
	font-size: 16px;
	line-height: 38px;
}
.posted-task {
	padding: 5px 10px;
}
.posted-task:after {
	position: relative;
	top: 6px;
	content: '';
	display: block;
	width: 1178px;
	height: 0px;
	margin: 0 auto;
	border-bottom: 1px dashed #e3e3e3;
	clear: both;
}
.posted-task:last-child {
	border-bottom: 1px solid #e3e3e3;
}
.posted-task:hover {
	background: #f5f5f5;
}
.posted-task:hover .task-thumbnail {
	display: block;
}
.task-title {
	display: inline-block;
	float: left;
	width: 50%;
}
.task-poster,.task-sumup,.task-check,.task-status,.task-fetch {
	display: inline-block;
	float: left;
	width: 10%;
}
.posted-task>a:first-child {
	font-size: 16px;
	line-height: 35px;
	text-decoration: none;
	color: #555;
}
.posted-task>span {
	font-size: 14px;
	line-height: 35px;
	color: #777;
}
.task-fetch {
	float: right;
	font-size: 14px;
	line-height: 35px;
	width: 8%;
	display: inline-block;
	color: #fff !important;
	text-align: center;
	background: #2399F4;
	border-radius: 5px;
	cursor: pointer;
	text-decoration: none;
}
.task-thumbnail {
	display: none;
	width: 50%;
	margin-bottom: 10px;
	font-size: 14px;
	line-height: 20px;
	color: #777;
}
.pages {
	width: 1200px;
	margin: 10px auto 0;
}
.pages:after {
	content: '';
	height: 0;
	display: block;
	clear: both;
	visibility: hidden;
}
.pages>span {
	font-size: 12px;
	line-height: 28px;
	color: #777;
	display: block;
	float: left;
}
.page-control {
	float: right;
	border: 1px solid #e3e3e3;
	border-radius: 3px;
}
.page-control>a {
	display: block;
	float: left;
	width: 26px;
	height: 26px;
	border-right: 1px solid #e3e3e3;
	background: #fff;
	text-align: center;
	text-decoration: none;
	font-size: 12px;
	line-height: 26px;
	color: #777;
}
.page-control>a:last-child {
	border: 0;
	width: auto;
	padding: 0 10px;
}
.page-control .active {
	background: #f5f5f5;
	font-weight: 700;
}
footer{
	margin-top: 30px;
    width:100%;
    border-top:1px solid #999999;
    text-align: center;
    background-color: #f3f3f3;
    padding:.01%;
}
footer>div {
	margin-top: 10px;
}
footer a,p,div {
	font-size: 12px;
	line-height: 18px;
	color: #666;
	text-decoration: none;
}

