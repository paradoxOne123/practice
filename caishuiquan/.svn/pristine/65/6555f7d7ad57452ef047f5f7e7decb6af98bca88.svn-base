* {
	padding: 0;
	margin: 0;
	list-style-type: none;
}
body{
    font:12px "microsoft yahei",Arial,Helvetica,sans-serif;
    color:#666;
    margin:0px;
}
ul,ol,li,p,h1,h2,h3,h4,h5,h6,dl,dt,dd{
    margin:0px;
    padding:0px;
    list-style:none;
}
a{
    color:#666;
    text-decoration:none;
}

.arrow {
	margin-left: 3px;
	position: relative;
	top: 2px;
	display: inline-block;
	border: 4px solid transparent;
	border-top-color: #555;
}
.clearfix:after {
	content: '';
	height: 0;
	visibility: hidden;
	display: block;
	clear: both;
}
/*这里是导航*/
.nav {
	width: 1200px;
	margin: 10px auto 0;
	box-sizing: border-box;
}
.nav:after {
	content: '';
	display: block;
	height: 0;
	visibility: hidden;
	clear: both;
}
.main-nav {
	position: relative;
	width: 18%;
	float: left;
	border: 1px solid #e3e3e3;
	border-right: 0;
	background: rgba(0,0,0,.01);
	box-sizing: border-box;
}
.main-nav .klass {
	padding: 15px 0 15px 15px;
	border-bottom: 1px dashed #e3e3e3;
	border-right: 1px solid #e3e3e3;
}
.main-nav .klass:last-child {
	border-bottom: 0;
}
.main-nav .klass>a {
	display: block;
	font-size: 16px;
	line-height: 28px;
	font-weight: 700;
	color: #555;
}
.main-nav .klass>span {
	font-size: 13px;
	line-height: 20px;
	color: #999;
}
.sub-nav {
	display: none;
	background: #fff;
	position: absolute;
	top: -1px;
	left: 100%;
	width: 720px;
	height: 396px;
	border: 1px solid #e3e3e3;
	border-left: 0;
	padding: 20px;
	z-index: 30;
}
.klass:hover {
	background: #fff;
	border-right: 0;
	border-left: 5px solid #00A85F;
}
.klass:hover>a,.klass:hover>span {
	position: relative;
	left: 10px;
}
.klass:hover>.sub-nav {
	display: block;
}
.sub-nav .content-box {
	width: 45%;
	margin-left: 5%;
	float: left;
	border-bottom: 1px dashed #e3e3e3;
	margin-bottom: 10px;
}
.content-box:after {
	content: '';
	height: 0;
	display: block;
	clear: both;
	visibility: hidden;
}
.content-box h4 {
	font-size: 14px;
	margin: 5px 0;
	padding-left: 10px;
	line-height: 18px;
	color: #555;
	border-left: 3px solid #00A85F;
}
.content-box li {
	padding: 8px 0;
	float: left;
}
.content-box li a {
	padding: 0 15px;
	border-right: 1px solid #f5f5f5;
	font-size: 12px;
	color: #777;
	text-decoration: none;
}
.content-box li:first-child a {
	padding-left: 0;
}
.content-box li a:hover {
	text-decoration: underline;
}
/*这里是slider*/
.slider {
	
	background: #e3e3e3;
}
.banner { 
	width: 58%;
	float: left;
	margin-left: 2%;
	height: 396px;
	position: relative;
	overflow: hidden;
}
.banner ul li { 
	float: left;
	height: 396px;
	list-style-type: none;
	overflow: hidden;
}
.banner ul li img {
	width: 100%;
	height: 100%;
}
.dots {
	text-align: center;
}
.dots>li {
	position: relative;
	top: -20px;
	display: inline-block;
	width: 10px;
	height: 10px;
	background: #eee;
	margin: 0 10px;
	cursor: pointer;
	border: 2px solid #eee;
	border-radius: 10px;
}
.dots>.active {
	background: #2399F4;
}
.unslider-arrow {
	display: none;
}
/*这里是上部滚动TOP视频*/
.top-video {
	margin-left: 2%;
	width: 20%;
	height: 396px;
	float: left;
	padding: 10px;
	border: 1px solid #e3e3e3;
	box-sizing: border-box;
}
.hot-heading h4 {
	font-size: 14px;
	line-height: 14px;
	color: #555;
	padding-bottom: 10px;
	border-bottom: 1px solid #f5f5f5;
}
.hotvideo-list a {
	display: block;
	padding: 7px 0;
	border-bottom: 1px solid #f5f5f5;
}
.hotvideo-list a:hover h5 {
	color: orange;
}
.hotvideo-list a:last-child {
	border: 0;
}
.hotvideo-list a img {
	width: 48%;
	float: left;
}
.hotvideo-list a div {
	width: 50%;
	margin-left: 2%;
	float: left;
	padding: 7px 0;
}
.hotvideo-list a div h5 {
	margin-bottom: 7px;
	font-size: 12px;
	line-height: 12px;
	color: #555;
	width: 100%;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}
.hotvideo-list a div p {
	font-size: 12px;
	line-height: 18px;
	margin-bottom: 5px;
	color: #555;
}
.hotvideo-list a div span {
	font-size: 12px;
	line-height: 16px;
	color: #999;
}
.hotvideo-list a div span em {
	color: orange;
}
/*这里是免费视频*/
.free-video {
	width: 1200px;
	margin: 20px auto 0;
}
.free-video:after {
	content: '';
	height: 0;
	visibility: hidden;
	clear: both;
	display: block;
}
.title {
	overflow: hidden;
}
.title h2 {
	float: left;
	font-size: 20px;
	line-height: 24px;
	color: #555;
	padding-left: 20px;
	margin : 8px 0;
	border-left: 5px solid #00A85F;
}
.title h2 span {
	font-size: 24px;
	line-height: 24px;
	color: #00A85F;
	padding : 8px 10px 8px 0px;
}
.title a {
	display: inline-block;
	float: right;
	font-size: 14px;
	line-height: 41px;
}
.title a:hover {
	text-decoration: underline;
}
.video-box {
	width: 20%;
	float: left;
	background: #fff;
	padding: 10px;
	box-sizing: border-box;
}
.video-box:hover {

}
.video-box a:first-child {
	width: 220px;
	height: 157px;
	display: block;
	position: relative;
}
.video-box:nth-child(2) a:first-child {
	background-image: url(../images/freevideo1.jpg);
}
.video-box:nth-child(3) a:first-child {
	background: url(../images/freevideo2.jpg);
}
.video-box:nth-child(4) a:first-child {
	background: url(../images/freevideo3.jpg);
}
.video-box:nth-child(5) a:first-child {
	background: url(../images/freevideo1.jpg);
}
.video-box:nth-child(6) a:first-child {
	background: url(../images/freevideo2.jpg);
}
.vip-video .video-box:nth-child(2) a:first-child {
	background-image: url(../images/vip1.jpg);
}
.vip-video .video-box:nth-child(3) a:first-child {
	background: url(../images/vip2.jpg);
}
.vip-video .video-box:nth-child(4) a:first-child {
	background: url(../images/vip3.jpg);
}
.vip-video .video-box:nth-child(5) a:first-child {
	background: url(../images/vip4.jpg);
}
.vip-video .video-box:nth-child(6) a:first-child {
	background: url(../images/vip5.jpg);
}
.video-box a img {
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%,-50%);
	width: 30px;
	height: 30px;
	padding: 8px;
	border: 4px solid #f0f0f0;
	border-radius: 60px;
	background: rgba(0,0,0,.6);
	opacity: .3;
}
.video-box a:hover img {
	opacity: 1;
}
.video-box a:nth-child(2) {
	font-size: 12px;
	line-height: 24px;
	color: #555;
}
.video-box p {
	margin-top: 5px;
}
.video-box p img {
	width: 15px;
	margin-right: 5px;
}
.video-box p span {
	font-size: 10px;
}
/*这里是老师和课程*/
.top-teacher {
	width: 1200px;
	margin: 20px auto 0;
	box-sizing: border-box;
}
.top-teacher:after {
	content: '';
	height: 0;
	visibility: hidden;
	clear: both;
	display: block;
}
.teacher-info {
	margin-top: 10px;
	border: 1px solid #f0f0f0;
}
.teacher-info:after {
	content: '';
	height: 0;
	visibility: hidden;
	clear: both;
	display: block;
}
.teacher-left {
	width: 30%;
	height: 360px;
	float: left;
}
.teacher-left>div {
	padding: 10px 10px 10px 20px;
	box-sizing: border-box;
	background: #f0f0f0;
	overflow: hidden;
}
.teacher-left>.active {
	background: #fff;
	padding-left: 17px;
	border-left: 3px solid #00A85F;
}
.teacher-left>div>img {
	width: 100px;
	height: 100px;
	float: left;
}
.teacher-left>div>div {
	margin-left: 120px;
}
.teacher-left>div>div>h4 {
	font-size: 16px;
	line-height: 30px;
	color: #555;
}
.teacher-left>div>div>p {
	margin: 5px 0;
	font-size: 14px;
	line-height: 20px;
	color: #777;
}
.teacher-right {
	width: 70%;
	height: 360px;
	float: left;
}
.teacher-right>.lesson {
	padding: 10px 10px 10px 20px;
	background: #fff;
	position: relative;
	height: 100%;
	box-sizing: border-box;
	display: none;
}
.teacher-right .active {
	display: block;
}
.lesson:after {
	content: '';
	height: 0;
	visibility: hidden;
	display: block;
	clear: both;
}
.lesson>img {
	position: absolute;
	left: 20px;
	top: 50%;
	transform: translateY(-50%);
	width: 70%;
}
.lesson>div {
	width: 25%;
	height: 100%;
	float: right;
	position: relative;
}
.lesson>div>h3 {
	font-size: 18px;
	line-height: 22px;
	margin: 10px 0;
	color: #555;
}
.lesson>div>p {
	font-size: 14px;
	line-height: 24px;
	margin-top: 10px;
}
.lesson>div>a {
	display: block;
	position: absolute;
	left: 0;
	bottom: 10px;
	padding: 5px 10px;
	background: #2399F4;
	border-radius: 5px;
	color: #fff;
	font-size: 14px;
}
.lesson>div> span {
	position: absolute;
	right: 0;
	bottom: 10px;
	font-size: 12px;
	line-height: 28px;
	color: #999;
}
/*这里是职业路径*/
.career-path {
	width: 1200px;
	margin: 20px auto 0;
}
.title>span {
	font-size: 14px;
	color: #777;
	margin-left: 10px;
	position: relative;
	top: 15px;
}
.career-path:after {
	content: '';
	height: 0;
	visibility: hidden;
	display: block;
	clear: both;
}
.lesson-pack {
	margin-top: 10px;
}
.lesson-pack:after {
	content: '';
	height: 0;
	visibility: hidden;
	display: block;
	clear: both;
}
.lesson-pack>div{
	width: 23%;
	margin: 0 1%;
	float: left;
	box-sizing: border-box;
	position: relative;
}
.lesson-pack>div:hover {
	animation: goup 0.3s;
	animation-fill-mode: forwards;
}
.lesson-pack>div:hover:after {
	position: absolute;
	top: -10px;
	left: 0;
	z-index: -1;
	content: '';
	width: 100%;
	height: 100%;
	margin-top: 10px;
	display: block;
	overflow: hidden;
	border-radius: 5px;
	box-shadow: 5px 5px 5px rgba(0,0,0,.2);
}
@keyframes goup {
from {top:0px;}
to {top:-8px;}
}
.lesson-pack>div>a {
	position: relative;
	display: block;
	width: 100%;
}
.lesson-pack>div>a>img {
	width: 100%;
	border-top-left-radius: 3px;
	border-top-right-radius: 3px;
}
.lesson-pack>div>a>p {
	position: absolute;
	left: 0;
	bottom: 3px;
	width: 100%;
	font-size: 14px;
	line-height: 30px;
	color: #fff;
	text-align: center;
	background: rgba(0,0,0,.3);
}
.lesson-pack>div>a>span {
	position: absolute;
	right: 5px;
	top: 5px;
	font-size: 12px;
	line-height: 25px;
	padding: 0 10px;
	color: #fff;
	text-align: center;
	background: #2399F4;
	border-radius: 5px;
}
.lesson-pack>div>div {
	border: 1px solid #e3e3e3;
	border-top: 0;
	margin-top: -3px;
	box-sizing: border-box;
	height: 110px;
	padding: 8px;
	background: #fff;
	border-bottom-left-radius: 3px;
	border-bottom-right-radius: 3px;
}
.lesson-pack>div>div>h4 {
	font-size: 14px;
	line-height: 20px;
}
.lesson-pack>div>div>p {
	font-size: 12px;
	line-height: 18px;
	margin-top: 5px;
}
/*这里是侧边栏*/
.instant-link {
	position: fixed;
	top: 50%;
	right: 0;
	z-index: 30;
}
.instant-link .icon {
	display: block;
	height: 40px;
	text-decoration: none;
	margin-bottom: 5px;
}
.icon>div {
	padding: 0 10px;
	background: #2399F4;
	border-radius: 10px;
}
.icon>div:hover {
	background: orange;
}
.icon-personal {
	position: absolute;
	top: 0;
	right: 0;
}
.icon-task {
	position: absolute;
	top: 50px;
	right: 0;
}
.icon-order {
	position: absolute;
	top: 100px;
	right: 0;
}
.icon-message {
	position: absolute;
	top: 150px;
	right: 0;
}
.icon-service {
	position: absolute;
	top: 200px;
	right: 0;
}
.icon:hover span {
	display: inline-block;
}
.icon>div>span {
	font-size: 16px;
	color: #fff;
	position: relative;
	top: -12.5px;
	display: none;
}
.icon>div>img {
	width: 25px;
	height: 25px;
	padding: 7.5px 0;
}
.unslider-wrap {
	position: relative;
}

.hot-list {
	width: 100%;
}
.hot-container {
	width: 1200px;
	border: 1px solid #e5e5e5;
	margin: 10px auto;
	padding: 0 10px;
	box-sizing: border-box;
}
.hot-container>h2 {
	margin:15px 0 15px;
	padding-left: 30px;
	border-left: 5px solid orange;
	font-size: 18px;
}
.hot-container>div {
	padding-bottom: 10px;
	border-top: 1px dashed #e5e5e5;
	overflow: hidden;
	text-align: center;
}
.hot-container>div>a {
	display: inline-block;
	width: 16.66%;
	float: left;
	font-size: 16px;
	line-height: 36px;
	color: #555;
	text-decoration: none;
}
.hot-container>div>a>img {
	position: relative;
	top: 12px;
	display: inline-block;
	width: 20px;
	height: 20px;
	padding: 8px;
	margin-right: 10px;
	background: #2399F4;
	border-radius: 20px;
}
.tasks,.offices {
	width: 100%;
}
.tasks-container,.ofiices-container {
	position: relative;
	box-sizing: border-box;
	width: 1200px;
	height: 444px;
	margin: 10px auto 0;
	border-top: 2px solid #2399F4;
	border-bottom: 2px solid #55B5FF;
	border-right: 2px solid #55B5FF;
}
.ofiices-container {
	border-top: 2px solid #FF7D3E;
	border-bottom: 2px solid #FFA561;
	border-right: 2px solid #FFA561;
}

.tasks-left {
	width: 20%;
	height: 100%;
    background: linear-gradient(to bottom,  #2399F4 0%,#55B5FF 100%);
	padding: 20px 0 0 0;
	box-sizing: border-box;
}
.offices-left {
	width: 20%;
	height: 100%;
    background: linear-gradient(to bottom,  #FF7D3E 0%,#FFA561 100%);
	padding: 20px 0 0 0;
	box-sizing: border-box;
}
.tasks-left>h2,.offices-left>h2 {
	padding-left: 20px;
	font-size: 30px;
	line-height: 60px;
	color: #fff;
	font-weight: normal;
}
.tasks-left>a,.offices-left>a {
	margin-left: 20px;
	display: inline-block;
	padding: 8px 15px;
	background: rgba(0,0,0,.2);
	font-size: 14px;
	color: #fff;
	text-decoration: none;
	border-radius: 5px;
}
.tasks-list,.offices-list {
	margin-top: 30px;
	text-align: right;
	list-style-type: none;
}
.offices-list {
}
.tasks-list li,.offices-list li {
	margin-bottom: 10px;
}
.tasks-list li:last-child,.offices-list li:last-child {
	margin-bottom: 0;
}
.tasks-list li a,.offices-list li a {
	display: inline-block;
	width: 120px;
	padding: 10px 0;
	background: rgba(255,255,255,.2);
	border: 1px solid #fff;
	border-right: 0;
	border-top-left-radius: 5px;
	border-bottom-left-radius: 5px;
	text-decoration: none;
	font-size: 14px;
	color: #fff;
	text-align: center;
}
.tasks-list-content,.offices-list-content {
	display: none;
	position: absolute;
	top: 0;
	left: 20%;
	width: 80%;
	height: 100%;
}
.tasks-list-content>.task-thumbnail,.office-thumbnail {
	position: relative;
	float: left;
	box-sizing: border-box;
	width: 47%;
	height: 33.33%;
	padding: 20px 0;
	margin: 0 0 0 3%;
	border-bottom: 1px dashed #e5e5e5;
	text-align: left;
	overflow: hidden;
}
.tasks-list-content>.task-thumbnail:nth-child(5),.tasks-list-content>.task-thumbnail:last-child,.office-thumbnail:nth-child(5),.office-thumbnail:last-child {
	border: 0;
}
.task-thumbnail h4 {
	font-size: 16px;
	line-height: 30px;
	color: #555;
}
.task-thumbnail .user-id {
	display: block;
	font-size: 14px;
	line-height: 26px;
	color: #999;
}
.task-thumbnail .money-num {
	font-size: 16px;
	line-height: 30px;
	color: #555;
}
.task-thumbnail .money-num span {
	font-size: 24px;
	line-height: 28px;
	color: orange;
}
.task-thumbnail .end-time {
	display: block;
	font-size: 14px;
	line-height: 26px;
	color: #999;
}
.task-thumbnail .offer-now,.office-thumbnail .check-now{
	position: absolute;
	right: 10px;
	bottom: 10px;
	padding: 8px 15px;
	color: #fff;
	background: orange;
	border-radius: 5px;
}
.office-thumbnail .check-now {
	background: #2399F4;
}
.office-thumbnail>img {
	width: 25%;
	float: left;
}
.office-thumbnail>div {
	float: left;
	width: 70%;
	margin-left: 5%;
}
.office-thumbnail>div>h4 {
	font-size: 16px;
	line-height: 24px;
	color: #555;
}
.office-thumbnail>div>span,.office-thumbnail>div>p {
	display: block;
	font-size: 13px;
	line-height: 24px;
	color: #999;
}
.office-thumbnail .recent span {
	font-size: 14px;
	line-height: 24px;
	color: orange;
}
footer{
	margin-top: 30px;
    width:100%;
    border-top:1px solid #999999;
    text-align: center;
    background-color: #f3f3f3;
    padding:.01%;
}
footer>div {
	margin-top: 10px;
}
footer a,p,div {
	font-size: 12px;
	line-height: 18px;
	color: #666;
	text-decoration: none;
}



