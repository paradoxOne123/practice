* {
    padding: 0;
    margin: 0;
    list-style-type: none;
    text-decoration: none;
}
.clearfix:after {
    content: '';
    height: 0;
    visibility: hidden;
    display: block;
    clear: both;
}
.maincontent-container {
    width: 1000px;
    margin: 15px auto 0;
}
.maincontent-left {
    width:72%;
    float: left;
    margin-right: 3%;
}
.maincontent-left>div {
    width: 100%;
    box-sizing: border-box;
    border: 1px solid #e3e3e3;
    padding: 15px;
    margin-bottom: 15px;
}
.maincontent-container h3 {
    padding-left: 15px;
    margin-bottom: 15px;
    font-size: 14px;
    line-height: 14px;
    border-left: 3px solid #2399f4;
}
.office-intro>div,.good-at>div,.service-link>div {
    border-top: 1px solid #f3f3f3;
}
.office-intro>div {
    position: relative;
}
.office-intro {
    padding-bottom: 30px !important;
}
.office-intro>div>img {
    display: block;
    float: left;
    margin-top: 15px;
    width: 20%;
    border: 1px dashed #e3e3e3;
    box-sizing: border-box;
}
.office-intro>div>div {
    display: block;
    float: left;
    margin-top: 15px;
    margin-left: 5%;
    width: 75%;
    height: 145px;
    overflow: hidden;
}
.office-intro>div>div>h4 {
    font-size: 16px;
    line-height: 16px;
    color: #333;
    margin-bottom: 15px;
}
.office-intro>div>div>p {
    font-size: 14px;
    line-height: 20px;
    color: #333;
    margin-bottom: 15px;
    text-indent: 2em;
}
.office-intro>div>a {
    position: absolute;
    right: 10px;
    bottom: -20px;
    text-decoration: none;
    font-size: 12px;
    color: orange;
}
.good-at>div>img {
    width: 17%;
    float: left;
    margin-top: 15px;
    margin-right: 3%;
}
.good-at>div>ul {
    width: 80%;
    float: left;
    margin-top: 15px;
}
.good-at>div>ul>li {
    margin-right: 15px;
    margin-bottom: 15px;
    float: left;
}
.good-at>div>ul>li>i,.good-at>div>ul>li>span {
    color: #555;
}
.service-link>div {
    padding-top: 15px;
}
.service-link>div>a {
    display: block;
    float: left;
    box-sizing: border-box;
    width: 25%;
    padding: 0 10px 10px 10px;
    margin-bottom: 15px;
    position: relative;
}
.service-link>div>a:hover {
    animation: goup 0.5s;
    animation-fill-mode: forwards;
    box-shadow: 0 2px 5px rgba(0,0,0,.2);
}
.service-link>div>a:hover p {
    color: orange;
}
@keyframes goup {
    from {top:0px;}
    to {top:-5px;}
}
.service-link>div>a>img {
    width: 100%;
    margin-bottom: 10px;
}
.service-link>div>a>p {
    font-size: 14px;
    height: 18px;
    line-height: 18px;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    color: #333;
}
.maincontent-right {
    width: 25%;
    float: left;
}
.office-crew {
    width: 100%;
    box-sizing: border-box;
    border: 1px solid #e3e3e3;
    padding: 15px;
    margin-bottom: 15px; 
    background: #fafafa; 
}
.office-crew>div {
    width: 100%;
    border-top: 1px solid #f5f5f5;
}
.crew-cell {
    height: 125px;
    padding: 10px 0;
    border-bottom: 1px solid #f3f3f3;
}
.crew-cell:last-child {
    border: 0 ;
}
.office-crew>div>div>a {
    display: block;
    width: 30%;
    float: left;
    margin-right: 3%;
}
.office-crew>div>div>a>img {
    width: 100%;
}
.office-crew>div>div>div {
    width: 67%;
    float: left;
}
.office-crew>div>div>div>h4 {
    font-size: 14px;
    color: #333;
    margin-bottom: 6px;
}
.office-crew>div>div>div>p {
    font-size: 12px;
    color: #555;
    margin-bottom: 6px;    
}
.office-crew>div>div>div>span {
    height: 52px;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;  
    -webkit-line-clamp: 3; 
    -webkit-box-orient: vertical;   
    font-size: 12px;
    line-height: 18px;
    color: #555;
}